{
  "name": "sparknetworks-challenge",
  "version": "0.1.0",
  "description": "Filtering search match",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pichoncoder/spark-networks-challenge.git"
  },
  "homepage": "./",
  "proxy": "http://localhost:3001",
  "author": "Manuel Sanchez",
  "license": "MIT",
  "scripts": {
    "watch-node": "nodemon ./server/dist/server.js",
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start": "concurrently -k -p \"[{name}]\" -n \"Node,React,\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run start:server\" \"npm run start:client\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w  --project ./server/tsconfig.json",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "build:ts": "tsc --project ./server/tsconfig.json",
    "build:clean": "./node_modules/.bin/rimraf ./dist",
    "build": "yarn build:clean && yarn build:ts",
    "start:server": "yarn build && node ./dist/server/src/server.js | pino-colada",
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "test:client": "react-scripts test",
    "eject:client": "react-scripts eject",
    "server:client": "node-env-run server --exec nodemon | pino-colada"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/morgan": "^1.7.35",
    "@types/node": "12.0.10",
    "@types/rc-slider": "^8.6.4",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-async-router": "^0.1.15",
    "lodash": "^4.17.5",
    "moment": "^2.24.0",
    "mongoose": "^5.4.2",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "ramda": "^0.26.1",
    "rc-slider": "^8.6.13",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.4.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/lusca": "^1.5.0",
    "@types/node": "12.0.10",
    "@types/node-sass": "^4.11.0",
    "@types/request": "^2.47.0",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "express-pino-logger": "^4.0.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^1.4.4",
    "rimraf": "^2.6.3",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
